        <Style x:Key="ExpandButton" TargetType="ToggleButton">
            <Setter Property="Width" Value="16" />
            <Setter Property="Height" Value="76" />
            <Setter Property="Focusable" Value="False" />
            <Setter Property="SnapsToDevicePixels" Value="True" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ToggleButton">
                        <Grid SnapsToDevicePixels="True">
                            <Path
                                x:Name="back"
                                Width="16"
                                Height="76"
                                Data="M0 0 L16 6 L16 70 L0 76 Z"
                                Fill="#0396B0" />
                            <Path
                                x:Name="arrow"
                                Width="16"
                                Height="16"
                                Data="M11 2 L5 8 L11 14"
                                RenderTransformOrigin=".5 .5"
                                Stroke="#FFFFFF"
                                StrokeThickness="1">
                                <Path.RenderTransform>
                                    <RotateTransform x:Name="arrowTransform" Angle="180" CenterX=".5" CenterY=".5" />
                                </Path.RenderTransform>
                            </Path>
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="CheckStates">
                                    <VisualState x:Name="Checked">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="arrowTransform" Storyboard.TargetProperty="Angle">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="0" />
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Unchecked">
                                        <Storyboard>
                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetName="arrowTransform" Storyboard.TargetProperty="Angle">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="180" />
                                            </DoubleAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="Indeterminate" />
                                </VisualStateGroup>

                            </VisualStateManager.VisualStateGroups>
                        </Grid>

                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="CircleListBoxItem" TargetType="ListBoxItem">
            <Setter Property="SnapsToDevicePixels" Value="True" />
            <Setter Property="Foreground" Value="#ffffff" />
            <Setter Property="Width" Value="88" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ListBoxItem">
                        <Border
                            x:Name="Border1"
                            Background="Transparent"
                            SnapsToDevicePixels="True">
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Border
                                    x:Name="line1"
                                    Grid.Row="0"
                                    Grid.Column="0"
                                    Height="4"
                                    Background="#17557F"
                                    BorderThickness="0" />
                                <Border
                                    x:Name="carBorder"
                                    Grid.Row="0"
                                    Grid.Column="1"
                                    Width="16"
                                    Height="16"
                                    Background="Transparent"
                                    BorderBrush="#17557F"
                                    BorderThickness="4"
                                    CornerRadius="8"
                                    FocusVisualStyle="{x:Null}"
                                    SnapsToDevicePixels="True">
                                    <!--  WPF 默认会抗锯齿，所以会导致圆的内部有一些边框，这是在显示上解决这个问题的方案之一，具体可以看 https://stackoverflow.com/questions/18581204/how-to-fix-empty-space-between-a-border-and-a-background-in-button-with-rounded  -->
                                    <Border
                                        x:Name="aliased"
                                        Margin="-1"
                                        BorderBrush="{Binding RelativeSource={RelativeSource AncestorType=Border, Mode=FindAncestor}, Path=BorderBrush}"
                                        BorderThickness="2"
                                        CornerRadius="{Binding RelativeSource={RelativeSource AncestorType=Border, Mode=FindAncestor}, Path=CornerRadius}"
                                        Visibility="Hidden" />
                                </Border>
                                <Border
                                    x:Name="line2"
                                    Grid.Row="0"
                                    Grid.Column="2"
                                    Height="4"
                                    Background="#17557F"
                                    BorderThickness="0" />


                                <ContentPresenter
                                    Grid.Row="1"
                                    Grid.Column="0"
                                    Grid.ColumnSpan="3"
                                    Margin="0,6,0,0"
                                    HorizontalAlignment="Center" />
                            </Grid>
                            <VisualStateManager.VisualStateGroups>
                                <VisualStateGroup x:Name="SelectionStates">
                                    <VisualState x:Name="Unselected" />
                                    <VisualState x:Name="Selected">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="aliased" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{x:Static Visibility.Visible}" />
                                            </ObjectAnimationUsingKeyFrames>

                                            <ColorAnimationUsingKeyFrames Storyboard.TargetName="carBorder" Storyboard.TargetProperty="Background.(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames Storyboard.TargetName="carBorder" Storyboard.TargetProperty="BorderBrush.(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                    <VisualState x:Name="SelectedUnfocused">
                                        <Storyboard>
                                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="aliased" Storyboard.TargetProperty="Visibility">
                                                <DiscreteObjectKeyFrame KeyTime="0" Value="{x:Static Visibility.Visible}" />
                                            </ObjectAnimationUsingKeyFrames>

                                            <ColorAnimationUsingKeyFrames Storyboard.TargetName="carBorder" Storyboard.TargetProperty="Background.(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames Storyboard.TargetName="carBorder" Storyboard.TargetProperty="BorderBrush.(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextBlock.Foreground).(SolidColorBrush.Color)">
                                                <EasingColorKeyFrame KeyTime="0" Value="#13FFF5" />
                                            </ColorAnimationUsingKeyFrames>
                                        </Storyboard>
                                    </VisualState>
                                </VisualStateGroup>

                            </VisualStateManager.VisualStateGroups>
                        </Border>

                        <ControlTemplate.Triggers>
                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Self}, Converter={StaticResource IsLastItemInContainerConverter}}" Value="True">
                                <Setter TargetName="line2" Property="Visibility" Value="Hidden" />
                            </DataTrigger>
                            <Trigger Property="ItemsControl.AlternationIndex" Value="0">
                                <Setter TargetName="line1" Property="Visibility" Value="Hidden" />
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="CircleListBox" TargetType="ListBox">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="AlternationCount" Value="2147483647" />
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <StackPanel Orientation="Horizontal" />
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate>
                        <ScrollViewer
                            HorizontalScrollBarVisibility="Auto"
                            Style="{StaticResource CustomScrollViewerStyle}"
                            VerticalScrollBarVisibility="Auto">
                            <ItemsPresenter Width="{TemplateBinding Width}" SnapsToDevicePixels="True" />
                        </ScrollViewer>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="ItemContainerStyle" Value="{StaticResource CircleListBoxItem}" />
        </Style>



    </UserControl.Resources>

    <StackPanel Background="#061932">

        <ListBox
            Margin="20"
            DisplayMemberPath="Title"
            ItemsSource="{Binding Source={StaticResource Datas}}"
            Style="{StaticResource DeviceListBox}" />

        <ListBox
            Margin="20"
            DisplayMemberPath="Title"
            ItemsSource="{Binding Source={StaticResource Datas}}"
            Style="{StaticResource StationListBox}" />

        <ListBox
            Margin="20"
            DisplayMemberPath="Title"
            ItemsSource="{Binding Source={StaticResource Datas}}"
            Style="{StaticResource ButtonListBox}" />


        <ListBox
            Margin="20"
            ItemsSource="{Binding Source={StaticResource Datas}}"
            SelectedItem="{Binding ElementName=ListBoxDemoView, Path=SelectedsSchedule}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <RadioButton
                        Content="{Binding Title}"
                        GroupName="tt"
                        IsChecked="{Binding IsSelected, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type ListBoxItem}}}" />
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto" />
                <ColumnDefinition Width="Auto" />
            </Grid.ColumnDefinitions>
            <ListBox
                Margin="0"
                ItemsSource="{Binding Source={StaticResource Datas}}"
                Style="{StaticResource CctvListBox}">
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>

                            <Image
                                Width="18"
                                Height="18"
                                Margin="8,0"
                                Source="{StaticResource Home}">
                                <Image.Style>
                                    <Style TargetType="Image">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ListBoxItem}, Path=IsSelected}" Value="True">
                                                <Setter Property="Source" Value="{StaticResource HomeSelected}" />
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </Image.Style>
                            </Image>
                            <TextBlock
                                x:Name="title"
                                Grid.Column="1"
                                Width="0"
                                Text="{Binding Title}">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=ListBoxItem}, Path=IsSelected}" Value="True">
                                                <Setter Property="Foreground" Value="#05D0F4 " />
                                            </DataTrigger>
                                            <DataTrigger Binding="{Binding ElementName=expandButton, Path=IsChecked}" Value="True">
                                                <DataTrigger.EnterActions>
                                                    <BeginStoryboard>
                                                        <Storyboard>
                                                            <DoubleAnimationUsingKeyFrames BeginTime="0" Storyboard.TargetProperty="Width">
                                                                <EasingDoubleKeyFrame KeyTime="0:0:0.18" Value="200" />
                                                            </DoubleAnimationUsingKeyFrames>
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </DataTrigger.EnterActions>
                                                <DataTrigger.ExitActions>
                                                    <BeginStoryboard>
                                                        <Storyboard>
                                                            <DoubleAnimationUsingKeyFrames BeginTime="0" Storyboard.TargetProperty="Width">
                                                                <EasingDoubleKeyFrame KeyTime="0:0:0.15" Value="0" />
                                                            </DoubleAnimationUsingKeyFrames>
                                                        </Storyboard>
                                                    </BeginStoryboard>
                                                </DataTrigger.ExitActions>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
            <Border
                Grid.Column="1"
                BorderBrush="#0396B0"
                BorderThickness="2,0,0,0" />
            <ToggleButton
                x:Name="expandButton"
                Grid.Column="1"
                HorizontalAlignment="Left"
                Style="{StaticResource ExpandButton}" />
        </Grid>

        <ListBox
            Margin="20"
            ItemsSource="{Binding Source={StaticResource Datas}}"
            Style="{StaticResource CircleListBox}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Title}" />
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>

        <Border
            Width="60"
            Height="60"
            Margin="20"
            Background="Aqua"
            BorderBrush="Aqua"
            BorderThickness="10"
            CornerRadius="30">
            <Border
                Margin="-1"
                Background="Aqua"
                BorderBrush="Aqua"
                BorderThickness="10"
                CornerRadius="30" />
        </Border>

    </StackPanel>
</UserControl>

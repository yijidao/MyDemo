<rtu:ReactiveUserControl x:Class="ReactiveUIDemo.PIDSView"
                    x:TypeArguments="local:PIDSViewModel"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                    xmlns:local="clr-namespace:ReactiveUIDemo"
                    xmlns:hc="https://handyorg.github.io/handycontrol"
                    xmlns:rtu="http://reactiveui.net"
                    mc:Ignorable="d" 
                    d:DesignHeight="450" d:DesignWidth="800"
                    >

    <Grid>
        <TabControl TabStripPlacement="Top" x:Name="tabControl" >
            <TabItem Header="PIDS信息发布">
                <local:PublishPIDSView />
            </TabItem>
            <TabItem Header="PIDS配置信息">

            </TabItem>
            <TabItem Header="PIDS信息操作">

            </TabItem>
            <TabItem Header="PIDS信息发布记录"></TabItem>
        </TabControl>
    </Grid>
</rtu:ReactiveUserControl>
